Hat Kapatma - Ãcretlendirme: Hat kapatma sÃ¼recindeki Ã¼cretlendirme sorunlarÄ±. ata text, etiket formatinda olsun:\n\n${combinedText}`;
    
    textarea.value = searchText;
    textarea.dispatchEvent(new Event('input', { bubbles: true }));
    
    const enterEvent = new KeyboardEvent('keydown', {
    bubbles: true, 
    cancelable: true, 
    key: 'Enter', 
    keyCode: 13
});
    textarea.dispatchEvent(enterEvent);
    
    // CevabÄ± bekle - 20 saniye (5 yorum iÃ§in daha uzun sÃ¼re)
    await new Promise(resolve => setTimeout(resolve, 20000));
    
    // Not: Cevap span'inin seÃ§icisini gÃ¼ncellemeniz gerekebilir
    const answerSpan = document.querySelector('span[_ngcontent-ng-c2231547002]');
    if (answerSpan) {
        const answer = answerSpan.textContent;
        console.log(`Ä°Ålendi: ${entries.length} yorum`);
        return answer;
    } else {
        console.error("Cevap span'i bulunamadÄ±");
        return null;
    }
}

// TÃ¼m girdileri 5'erli gruplar halinde iÅleyen ana fonksiyon
async function processAllEntries() {
    for (let i = 0; i < csvData.length; i += 5) {
        const group = csvData.slice(i, Math.min(i + 5, csvData.length));
        const result = await processEntryGroup(group);
        if (result) {
            results.push(result);
        }

        console.log(`Ä°Ålenen yorum sayÄ±sÄ±: ${i + group.length}`);

        // Her grup iÅleminden sonra 30 saniye bekle, son grup deÄilse
        if (i + 5 < csvData.length) {
            console.log("Bir grup iÅlendi, 30 saniye bekleniyor...");
            await new Promise(resolve => setTimeout(resolve, 30000));
        }
    }
    
    console.log("TÃ¼m iÅlemler tamamlandÄ±. SonuÃ§lar:");
    console.log(results.join('\n\n'));
    
    // SonuÃ§larÄ± tek bir string olarak birleÅtir
    const combinedResults = results.join('\n\n');
    console.log("TÃ¼m sonuÃ§lar:");
    console.log(combinedResults);
}

// Ä°Ålemi baÅlat
processAllEntries();